generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model stopDetails {
    id Int @id @default(autoincrement())
    startTime String
    endTime String
    duration String
    location String
    reason String
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
